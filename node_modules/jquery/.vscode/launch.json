  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach Blink Browser",
      "type": "chrome",
      "request": "attach",
      "webRoot": "${workspaceFolder}",
      "urlFilter": "http://localhost:3000/*",
      "sourceMaps": true,
      "port": 9222,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
